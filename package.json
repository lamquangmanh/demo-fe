{
  "name": "core-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "prepare": "husky",
    "codegen": "graphql-codegen --require dotenv/config --config ./src/common/adapters/graphQL/codegen.ts dotenv_config_path=.env.local",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.5",
    "@apollo/client": "^3.9.10",
    "@apollo/experimental-nextjs-app-support": "^0.9.1",
    "@babel/highlight": "^7.24.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@tanstack/react-query": "^5.28.14",
    "@types/node": "18.17.0",
    "@types/react": "18.2.72",
    "@types/react-dom": "18.2.22",
    "antd": "^5.15.4",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "13.4.12",
    "graphql": "^16.8.1",
    "i18next": "^23.10.1",
    "next": "13.5.6",
    "next-i18next": "^15.2.0",
    "postcss": "8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.1.0",
    "react-icons": "^5.0.1",
    "styled-components": "^6.1.8",
    "tailwindcss": "3.4.1",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-typed-document-node/core": "^3.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "typescript": "^5.4.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext js,jsx,mdx,ts,tsx src --fix",
      "npm run format",
      "git add ."
    ]
  }
}
